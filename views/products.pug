extends adminBase

block content
    main.main
        h3.heading-secondary.ma-bt-lg All Products
        
        h3
            table.table
                thead.thead-dark
                        tr

                        th(scope='col') #
                        th(scope='col') name
                        th(scope='col') Price
                        th(scope='col') Summery
                        th(scope='col') quantity
                            - var n = 1;
                            each tour in tours
                                tbody
                                    tr
                                    td= n++
                                    td= tour.name
                                    td= tour.price
                                    td= tour.description
                                    td= tour.quantity
                                    



